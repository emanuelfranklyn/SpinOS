--{ idioma: portugues brasileiro }--

function testar(n,s)
	for x=1,4 do
		for y=1,24 do
			sleep(0,1)
			n.playNote(x,y)
		end
	end
	sleep(1)
	s.speak("testado com sucesso!",5000,"pt")
end

a = peripheral.wrap("right")
b = peripheral.wrap("left")
testar(a,b)