function teste()
sn, hmsg = http.checkURL("https://github.com")
if sn then
return true
elseif not sn then
error("Ocorreu um erro :"..hmsg)
os.pullEvent()
os.reboot()
end
end

function get(dono,repo,arquivo)
if teste() then
site2 =  "https://raw.githubusercontent.com/"..dono.."/"..repo.."/master/"..arquivo
cont = http.get(site2)
contread = cont.readAll()
cont.close()
return contread
else
teste()
end
end
function edit()
http.post("")
end
