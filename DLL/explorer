local function carregarapis()
os.unloadAPI("msgboxapi")
os.loadAPI("msgboxapi")
end

function taskbar()
xj,yj = term.getSize()
paintutils.drawFilledBox(1,yj,xj,yj,colors.gray)
end

carregarapis()
taskbar()
term.setCursorPos(1,1)